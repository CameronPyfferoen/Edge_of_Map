{
  "name": "edge-of-the-map",
  "version": "0.0.1",
  "description": "Game description here",
  "author": "Not professor berrier",
  "main": "index.html",
  "repository": "UWStout/edge-of-the-map",
  "scripts": {
    "sfx": "audiosprite --loop running --output assets/audio/sounds assets/audio/*.wav",
    "recode": "run-s recode1 recode2 recode3 recode4 recode5 recode6 recode7 recode8",
    "recode1": "audiosprite -i 1 -g 0 --output assets/audio/explosion assets/audio/SFX/q009/explosion.ogg",
    "recode2": "audiosprite -i 1 -g 0 --output assets/audio/grenade assets/audio/SFX/q009/grenade.ogg",
    "recode3": "audiosprite -i 1 -g 0 --output assets/audio/pirate1uf assets/audio/Music/pirate1uf.ogg",
    "recode4": "audiosprite -i 1 -g 0 --output assets/audio/Pirate1_Theme1 assets/audio/Music/Pirate1_Theme1.wav",
    "recode5": "audiosprite -i 1 -g 0 --output assets/audio/thunderchild assets/audio/thunderchild.wav",
    "recode6": "audiosprite -i 1 -g 0 --output assets/audio/weapswitch assets/audio/SFX/q009/weapswitch.ogg",
    "recode7": "audiosprite -i 1 -g 0 --output assets/audio/quaddamage_out assets/audio/SFX/q009/quaddamage_out.ogg",
    "recode8": "audiosprite -i 1 -g 0 --output assets/audio/quaddamage_shoot assets/audio/SFX/q009/quaddamage_shoot.ogg",
    "dev": "run-s clean dev-cmd",
    "prod": "run-s clean prod-cmd",
    "deploy": "run-s clean deploy-cmd",
    "package": "run-s clean sfx nwjs-cmd package-cmd",
    "packagetest": "run-s clean sfx nwjs-cmd packagetest-cmd",
    "clean": "del-cli -f dist/* packaged/*",
    "dev-cmd": "webpack --config webpack.dev.config.js",
    "prod-cmd": "webpack --config webpack.prod.config.js",
    "deploy-cmd": "webpack --config webpack.deploy.config.js",
    "nwjs-cmd": "cross-env NWJS=true webpack --config webpack.deploy.config.js",
    "package-cmd": "babel-node packager.js build",
    "packagetest-cmd": "run-script-os",
    "packagetest-cmd:darwin": "nwbuild -p osx64 -r .",
    "packagetest-cmd:win32": "nwbuild -p win64 -r .",
    "test": "eslint ./src/**/**.js"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "audiosprite": "^0.7.1",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "browser-sync": "^2.26.3",
    "browser-sync-webpack-plugin": "^2.2.2",
    "cpy-cli": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "del-cli": "^1.1.0",
    "eslint": "^5.4.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "expose-loader": "^0.7.5",
    "file-loader": "^2.0.0",
    "npm-run-all": "^4.1.3",
    "nw-builder": "^3.5.4",
    "run-script-os": "^1.0.3",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.17.1",
    "webpack-command": "^0.4.1"
  },
  "dependencies": {
    "phaser-ce": "^2.11.0",
    "sfx": "^0.1.1",
    "webfontloader": "^1.6.28"
  },
  "window": {
    "title": "Edge of the Map",
    "icon": "./assets/images/icon.png",
    "toolbar": false,
    "frame": true,
    "fullscreen": false
  }
}
